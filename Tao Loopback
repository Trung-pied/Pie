---
- name: Set  loopback
 host: all
 gather_facts: no
 tasks:
  - name: Set loopback IPv4 address
    ios_13_interface:
      name: loopback {{item.number}}
      ipv4: 10.0.{{item.number}}.1/24
    with_item:
     -{ number: 1}
     -{ number: 2}
     -{ number: 3}
     -{ number: 4}
  - name: Show ip
    ios_command:
     command:
      - show ip int br
    register: show_ip
  - debug: var=show_ip.stdout_lines  
 
